{
  "expo": {
    "name": "Bean Bye",
    "slug": "beanie-farewell",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "beanie-farewell",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1025"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beanbye.app",
      "buildNumber": "13",
      "infoPlist": {
        "NSCameraUsageDescription": "Bean Bye uses your camera to photograph and identify your Beanie Babies.",
        "NSPhotoLibraryUsageDescription": "Select pictures of your Beanie Babies for identification.",
        "NSPhotoLibraryAddUsageDescription": "Save Farewell Certificates to your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Bean Bye uses your camera to photograph and identify your Beanie Babies.",
          "photosPermission": "Select pictures of your Beanie Babies for identification."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Save Farewell Certificates to your photo library.",
          "savePhotosPermission": "Save Farewell Certificates to your photo library."
        }
      ],
      "expo-router"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a900df5c-4cfd-410f-b083-b3eeb2198b58"
      }
    },
    "owner": "jnoland"
  }
}
